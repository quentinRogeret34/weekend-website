{
    "name": "ternel/weekend",
    "type": "project",
    "license": "proprietary",
    "description": "Est-ce que c'est bient√¥t le weekend ? .fr",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "symfony/console": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/flex": "^1.15",
        "symfony/framework-bundle": "^5.3",
        "symfony/runtime": "^5.3",
        "symfony/twig-bundle": "^5.3",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "symfony/stopwatch": "^5.3",
        "symfony/web-profiler-bundle": "^5.3"
    },
    "config": {
        "platform": {
            "php": "7.4.21"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.3"
        }
    }
}
